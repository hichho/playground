{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "ANALYZE=1 umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "ahooks": "^2.10.12",
    "antd": "^4.17.1",
    "qs": "^6.10.1",
    "react": "17.x",
    "react-dom": "17.x",
    "react-helmet": "^6.1.0",
    "umi": "^3.5.20"
  },
  "umi-request": "1.2.19",
  "devDependencies": {
    "@types/qs": "^6.9.7",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.5.20",
    "antd-mobile": "latest",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.0",
    "typescript": "^4.1.2",
    "yorkie": "^2.0.0"
  }
}
